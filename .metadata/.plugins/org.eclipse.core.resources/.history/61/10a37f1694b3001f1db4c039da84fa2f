#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int number1 = 10, number2 = 20, temp;
	temp = number1;
	number1 = number2;
	number2 = temp;
	printf("Number 1: %d || Number 2 = %d", number1,number2);

	return EXIT_SUCCESS;
}
